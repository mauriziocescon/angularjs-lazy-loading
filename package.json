    "@types/angular-material": "1.1.72",
    "@types/angular-mocks": "1.7.0",
    "@types/angular-sanitize": "1.7.0",
    "@types/angular-translate": "2.16.2",
    "@types/angular-ui-router": "1.1.40",
    "@types/faker": "5.1.3",
