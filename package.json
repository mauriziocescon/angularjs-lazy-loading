{
  "name": "angular-1-x-lazy-loading",
  "version": "1.0.0",
  "description": "A demo application for lazy loading of angular 1.x components",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com>",
  "homepage": "https://be.linkedin.com/in/mauriziocescon",
  "private": false,
  "engines": {
    "node": ">=20.9.0",
    "npm": ">=10.1.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "serve:lazy": "npm run serve --prefix modules/lazy",
    "build:lazy": "npm run build --prefix modules/lazy",
    "test:lazy": "npm run test --prefix modules/lazy",
    "serve": "npm run serve:lazy && webpack serve --progress --color --env name=dev --config webpack.config.js | npm run json-server-watch",
    "build": "npm run build:lazy && webpack --progress --profile --color --env name=prod",
    "pretest": "npm run test:lazy && webpack --progress --profile --color --env name=test",
    "test": "karma start karma.conf.js",
    "bundle-analyzer:lazy": "npm run bundle-analyzer --prefix modules/lazy",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/angularjs-lazy-loading"
  },
  "keywords": [
    "angularjs",
    "cescon",
    "es2017",
    "json-server",
    "karma",
    "material design",
    "maurizio",
    "mock-server",
    "oclazyload",
    "sass",
    "travis",
    "typescript",
    "vendor",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "angular": "1.8.3",
    "angular-animate": "1.8.3",
    "angular-aria": "1.8.3",
    "angular-dynamic-locale": "0.1.32",
    "angular-i18n": "1.8.3",
    "angular-material": "1.2.5",
    "angular-messages": "1.8.3",
    "angular-mocks": "1.8.3",
    "angular-sanitize": "1.8.3",
    "angular-stats": "1.2.4",
    "angular-translate": "2.19.1",
    "angular-translate-loader-static-files": "2.19.1",
    "angular-translate-storage-local": "2.19.1",
    "angular-ui-router": "0.4.3",
    "oclazyload": "1.1.0",
    "parse-link-header": "2.0.0",
    "querystring-es3": "0.2.1",
    "roboto-fontface": "0.10.0",
    "stacktrace-js": "2.0.2",
    "url": "0.11.3"
  },
