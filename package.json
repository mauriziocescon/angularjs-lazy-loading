{
  "name": "angular-1-x-lazy-loading",
  "version": "1.0.0",
  "description": "A demo application for lazy loading of angular 1.x components",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com> (http://www.mauriziocescon.com)",
  "homepage": "http://www.mauriziocescon.com",
  "private": false,
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "serve:lazy": "npm run serve --prefix modules/lazy",
    "build:lazy": "npm run build --prefix modules/lazy",
    "test:lazy": "npm run test --prefix modules/lazy",
    "serve": "npm run serve:lazy && webpack-dev-server --d --progress --profile --color --env name=dev | npm run json-server-watch",
    "build": "npm run build:lazy && webpack --progress --profile --color --env name=prod",
    "pretest": "npm run test:lazy && webpack --progress --profile --color --env name=test",
    "test": "karma start karma.conf.js",
    "bundle-analyzer:lazy": "npm run bundle-analyzer --prefix modules/lazy",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/angularjs-lazy-loading"
  },
  "keywords": [
    "angularjs",
    "cescon",
    "es2017",
    "json-server",
    "karma",
    "material design",
    "maurizio",
    "mock-server",
    "oclazyload",
    "sass",
    "travis",
    "typescript",
    "vendor",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "@babel/polyfill": "7.12.1",
    "angular": "1.8.2",
    "angular-animate": "1.8.2",
    "angular-aria": "1.8.2",
    "angular-dynamic-locale": "0.1.32",
    "angular-i18n": "1.8.2",
    "angular-material": "1.2.1",
    "angular-messages": "1.8.2",
    "angular-mocks": "1.8.2",
    "angular-sanitize": "1.8.2",
    "angular-stats": "1.2.4",
    "angular-translate": "2.18.3",
