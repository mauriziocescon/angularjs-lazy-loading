{
  "name": "angular-1-x-lazy-loading",
  "version": "1.0.0",
  "description": "A demo application for lazy loading of angular 1.x components",
  "author": "Maurizio Cescon <maurizio.cescon@gmail.com> (http://www.mauriziocescon.com)",
  "homepage": "http://www.mauriziocescon.com",
  "private": false,
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production npm run json-server",
    "json-server": "node ./mock-server/server.js",
    "json-server-watch": "nodemon --ext js --watch ./mock-server ./mock-server/server.js",
    "serve:lazy": "npm run serve --prefix modules/lazy",
    "build:lazy": "npm run build --prefix modules/lazy",
    "test:lazy": "npm run test --prefix modules/lazy",
    "serve": "npm run serve:lazy && webpack-dev-server --d --progress --profile --colors --env.name=dev | npm run json-server-watch",
    "build": "npm run build:lazy && webpack --progress --profile --colors --env.name=prod",
    "pretest": "npm run test:lazy && webpack --progress --profile --colors --env.name=test",
    "test": "karma start karma.conf.js",
    "bundle-analyzer:lazy": "npm run bundle-analyzer --prefix modules/lazy",
    "bundle-analyzer": "webpack-bundle-analyzer ./dist/stats.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriziocescon/lazy-loading"
  },
  "keywords": [
    "angularjs",
    "cescon",
    "es2017",
    "json-server",
    "karma",
    "material design",
    "maurizio",
    "mock-server",
    "oclazyload",
    "sass",
    "travis",
    "typescript",
    "vendor",
    "webpack"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/brentertz/scapegoat/blob/master/LICENSE-MIT"
    }
  ],
  "dependencies": {
    "angular": "1.8.0",
    "angular-animate": "1.7.9",
    "angular-aria": "1.7.9",
    "angular-dynamic-locale": "0.1.32",
    "angular-i18n": "1.7.9",
    "angular-material": "1.1.21",
    "angular-messages": "1.7.9",
    "angular-mocks": "1.7.9",
    "angular-sanitize": "1.7.9",
    "angular-stats": "1.2.4",
    "angular-translate": "2.18.2",
    "angular-translate-loader-static-files": "2.18.2",
    "angular-translate-storage-local": "2.18.2",
    "angular-ui-router": "0.4.3",
    "babel-polyfill": "6.26.0",
    "oclazyload": "1.1.0",
    "parse-link-header": "1.0.1",
    "roboto-fontface": "0.10.0",
    "stacktrace-js": "2.0.2"
  },
  "devDependencies": {
    "@types/angular": "1.6.57",
    "@types/angular-animate": "1.5.10",
    "@types/angular-dynamic-locale": "0.1.34",
    "@types/angular-material": "1.1.71",
    "@types/angular-mocks": "1.7.0",
    "@types/angular-sanitize": "1.7.0",
    "@types/angular-translate": "2.16.2",
    "@types/angular-ui-router": "1.1.40",
    "@types/faker": "4.1.10",
    "@types/jasmine": "3.5.7",
    "@types/node": "12.7.3",
    "@types/oclazyload": "0.0.32",
    "@types/parse-link-header": "1.0.0",
    "@types/webpack": "4.41.6",
    "awesome-typescript-loader": "5.2.1",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "cross-env": "7.0.0",
    "css-loader": "3.4.2",
    "expose-loader": "0.7.5",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "faker": "4.1.0",
    "file-loader": "5.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jasmine-core": "3.5.0",
    "json-server": "0.16.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-firefox-launcher": "1.3.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "3.1.1",
    "karma-safari-launcher": "1.0.0",
    "karma-spec-reporter": "0.0.32",
    "node-sass": "4.13.1",
    "nodemon": "2.0.2",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
