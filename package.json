    "@babel/preset-env": "^7.23.3",
    "@types/angular": "^1.8.8",
    "@types/angular-animate": "^1.5.14",
    "@types/angular-dynamic-locale": "0.1.34",
    "@types/angular-material": "^1.1.76",
    "@types/angular-mocks": "^1.7.4",
    "@types/angular-sanitize": "^1.8.4",
    "@types/angular-translate": "^2.19.3",
    "@types/angular-ui-router": "^1.1.44",
    "@types/jasmine": "^5.1.2",
    "@types/node": "^20.9.0",
    "@types/oclazyload": "0.0.32",
    "@types/parse-link-header": "^2.0.3",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^8.53.0",
    "eslint-webpack-plugin": "^4.0.1",
    "expose-loader": "^5.0.0",
    "faker": "^5.5.3",
