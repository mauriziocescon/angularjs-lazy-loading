{
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": ".",
    "checkJs": false,
    "diagnostics": true,
    "declaration": true,
    "declarationDir": "interfaces",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "module": "es2022",
    "moduleResolution": "bundler",
    "paths": {
      "icore": [
        "../interfaces/core"
      ]
    },
    "pretty": true,
    "removeComments": false,
    "sourceMap": true,
    "strict": true,
    "target": "es2022",
    "types": [
      "jasmine",
      "node"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
